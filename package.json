{
  "name": "medusa-backend",
  "version": "0.0.1",
  "description": "Medusa project.",
  "author": "Ricardo W Bento <roicoroy@mercadoamigao.com>",
  "license": "MIT",
  "repository": "https://github.com/medusajs/medusa-starter-default.git",
  "keywords": [
    "sqlite",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "sened": "medusa seed -f ./data/seed.json",
    "build": "babel src -d dist --extensions \".ts,.js\"",
    "ts-build": "./node_modules/.bin/tsc -p tsconfig.json",
    "format": "./node_modules/.bin/prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "ts-start": "npm run ts-build && NODE_ENV=development node ./dist/main.js",
    "start:prod": "npm run build && NODE_ENV=production node dist/main",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch --detectOpenHandles",
    "test:cov": "NODE_ENV=test jest --coverage --detectOpenHandles",
    "start": "medusa develop",
    "develop": "medusa develop",
    "env-path": "ENV_PATH=/Users/ricardobento/Documents/STRAPI/MEDUSA/medusa-store/.env npm run start"
  },
  "dependencies": {
    "@medusajs/medusa": "^1.3.3",
    "@medusajs/medusa-cli": "^1.3.0",
    "@types/jsonwebtoken": "^8.5.8",
    "awilix": "^6.1.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "medusa-core-utils": "^1.1.31",
    "medusa-extender": "^1.7.1",
    "medusa-file-cloudinary": "^1.0.1",
    "medusa-fulfillment-manual": "^1.1.31",
    "medusa-interfaces": "^1.3.0",
    "medusa-marketplace": "^1.2.2",
    "medusa-payment-manual": "^1.0.12",
    "medusa-payment-stripe": "^1.1.38",
    "medusa-plugin-meilisearch": "^0.2.1",
    "medusa-plugin-nodemailer": "git+https://github.com/Blechlawine/medusa-plugin-nodemailer.git",
    "medusa-plugin-restock-notification": "0.0.32",
    "mongoose": "^6.1.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "prom-client": "^12.0.0",
    "reflect-metadata": "^0.1.13",
    "swagger-parser": "^10.0.3",
    "swagger-stats": "^0.99.2",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.14.5",
    "babel-preset-medusa-package": "^1.1.19",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  }
}
